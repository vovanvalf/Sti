<div class="row-fluid" ng-show="data.visible">
    <div ng-class="'span' + spanRight"></div>
    <div ng-class="'span' + spanMiddle">
        <div ng-class="data.cssClass">
            <button class="close" type="button" ng-click="close()">&times;</button>
            <strong ng-bind="data.title"></strong>&nbsp;<span ng-bind="data.msg"></span>
        </div>
    </div>
    <div ng-class="'span' + spanLeft"></div>
</div>
