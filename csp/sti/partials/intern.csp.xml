<?xml version="1.0" encoding="UTF-8"?>
<Export generator="Cache" version="25">
<CSP name="sti/partials/intern.csp" application="/csp/sti/" default="1"><![CDATA[
<div class="row-fluid">
	<div class="span4">
	</div>
	<div class="span4">
		<div class="accordion">
			<div class="accordion-university">
				<div class="accordion-heading">
					<a class="accordion-toggle" ng-bind="page.formCaption"></a>
				</div>
				<div class="accordion-body">
					<div class="accordion-inner">
						<form name="pageForm" ng-submit="page.submit()" style="margin: 0 0 10px 0;" >
							<div class="control-group">
						        <label class="control-label">Фамилия*</label>
						        <div class="controls">
						            <input class="span12" type="text" ng-model="page.intern.lastname" required></input>
						        </div>
						    </div>
						    <div class="control-group">
						        <label class="control-label">Имя*</label>
						        <div class="controls">
						            <input class="span12" type="text" ng-model="page.intern.firstname" required></input>
						        </div>
						    </div>
						    <div class="control-group">
						        <label class="control-label">Отчество*</label>
						        <div class="controls">
						            <input class="span12" type="text" ng-model="page.intern.middlename" required></input>
						        </div>
						    </div>
						    <div class="control-group">
						        <label class="control-label">Email*</label>
						        <div class="controls">
						            <input class="span12" type="email" ng-model="page.intern.email" required></input>
						        </div>
						    </div>
						    <div class="control-group">
						        <label class="control-label">Телефон*</label>
						        <div class="controls">
						            <input class="span12" type="text" ng-model="page.intern.phone" required></input>
						        </div>
						    </div>
						    <div class="control-group">
						        <label class="control-label">Skype*</label>
						        <div class="controls">
						            <input class="span12" type="text" ng-model="page.intern.skype" required></input>
						        </div>
						    </div>
						    <div class="control-group">
						        <label class="control-label">Уровень английского*</label>
						        <div class="controls">
						            <input class="span12" type="text" ng-model="page.intern.eng.value" required></input>
						        </div>
						    </div>
						    <div class="control-group">
						        <label class="control-label">Город*</label>
						        <div class="controls">
						            <input class="span12" type="text" ng-model="page.intern.city.name" required></input>
						        </div>
						    </div>
						    <div class="control-group">
						        <label class="control-label">Резюме*</label>
						        <div class="controls">
						            <input class="span12" type="text" ng-model="page.intern.resume" required></input>
						        </div>
						    </div>
						    <div class="control-group">
						        <label class="control-label">Согласие на рассылку*</label>
						        <div class="controls">
						            <input class="span12" type="text" ng-model="page.intern.agree" required></input>
						        </div>
						    </div>
						    <div class="control-group">
						        <label class="control-label">Опыт Cache*</label>
						        <div class="controls">
						            <input class="span12" type="text" ng-model="page.intern.cache" required></input>
						        </div>
						    </div>
						    <div class="control-group">
						        <label class="control-label">Опыт Ensemble*</label>
						        <div class="controls">
						            <input class="span12" type="text" ng-model="page.intern.ensemble" required></input>
						        </div>
						    </div>
						    <div class="control-group">
						        <label class="control-label">Опыт DeepSee*</label>
						        <div class="controls">
						            <input class="span12" type="text" ng-model="page.intern.deepsee" required></input>
						        </div>
						    </div>
						    <div class="control-group">
						        <label class="control-label">Оценка TET*</label>
						        <div class="controls">
						            <input class="span12" type="text" ng-model="page.intern.tet" required></input>
						        </div>
						    </div>
						    <div class="control-group">
						        <label class="control-label">Оценка PET*</label>
						        <div class="controls">
						            <input class="span12" type="text" ng-model="page.intern.pet.value" required></input>
						        </div>
						    </div>
						  
						    <div class="control-group">
						   		<label class="control-label">Университет*</label>
						        <div class="controls">
						        	<!-- Заполнение комбобокса данными из массива с универами -->
					            	<select class="span12" ng-model="page.intern.university.id" ng-options="university.id as university.fullName for university in page.universities" required>
					            	</select>
						        </div>
						    </div>
						    <div class="control-group">
					        	<div class="controls">
						    		<button type="submit" class="btn btn-success btn-small" ng-bind="page.formBtnSubmitName" ng-disabled="pageForm.$pristine"></button>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
		</div>	
	</div>
	<div class="span4">
	</div>
</div>
<!-- Инфо-сообщения -->
<stialert data="page.alert" span-right="4" span-middle="4" span-left="4"/>
]]></CSP>
</Export>
