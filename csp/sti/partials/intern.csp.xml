<?xml version="1.0" encoding="UTF-8"?>
<Export generator="Cache" version="25">
<CSP name="sti/partials/intern.csp" application="/csp/sti/" default="1"><![CDATA[

<div class="row-fluid">
<form name="pageForm" ng-submit="page.submit()" style="margin: 0 0 10px 0;" >
	<div class="span10">
	</div>
	<div class="span10">
		<div class="accordion">
			<div class="accordion-group">
				<div class="accordion-heading">
					<a class="accordion-toggle" ng-bind="'Стажёр'|localize"></a>
				</div>
				<div class="accordion-body">
					<div class="accordion-inner">
						 <div class="row-fluid">
							<div class="span4">
								<label class="control-label" ng-bind="'Имя'|localize">:</label>
								<input class="span12"  type="text" ng-model="page.intern.firstName" required></input>
								<label class="control-label" ng-bind="'Фамилия'|localize">:</label>
								<input class="span12"  type="text" ng-model="page.intern.lastName" required></input>
								<label class="control-label" ng-bind="'Отчество'|localize">:</label>
								<input class="span12"  type="text" ng-model="page.intern.middleName" required></input>
								<label class="control-label" ng-bind="'Город'|localize">:</label>
								<input class="span12"  type="text" ng-model="page.intern.city.name" required></input>
								<label class="control-label" ng-bind="'Университет'|localize">:</label>
								<select class="span12" ng-model="page.intern.university.id" ng-options="university.id as university.fullName for university in page.universities" required></select>
					    		<label class="control-label" ng-bind="'Стажировка'|localize">:</label>
								<select class="span12" ng-model="page.intern.internship.id" ng-options="internship.id as internship.name for internship in page.internships" required></select>
					    	</div>
							<div class="span4">
								<label class="control-label" ng-bind="'Email'|localize">:</label>
								<input class="span12"  type="text" ng-model="page.intern.email" required></input>
								<label class="control-label" ng-bind="'Телефон'|localize">:</label>
								<input class="span12"  type="text" ng-model="page.intern.phone" required></input>
							    <label class="control-label" ng-bind="'Skype'|localize">:</label>
								<input class="span12"  type="text" ng-model="page.intern.skype" required></input>
								<label class="control-label" ng-bind="'Резюме'|localize">:</label>
								<input class="span12"  type="text" ng-model="page.intern.resume" required></input>
                                <label class="control-label" ng-bind="'Согласие на рассылку'|localize">:</label>
								<input class="span12"  type="text" ng-model="page.intern.agree" required></input>
							</div>
							<div class="span4">
								<label class="control-label" ng-bind="'Уровень английского'|localize">:</label>
								<input class="span12"  type="text" ng-model="page.intern.eng.value" required></input>
								<label class="control-label" ng-bind="'Опыт Cache'|localize">:</label>
								<input class="span12"  type="text" ng-model="page.intern.cache" required></input>
						        <label class="control-label" ng-bind="'Опыт DeepSee'|localize">:</label>
								<input class="span12"  type="text" ng-model="page.intern.deepsee" required></input>	
								<label class="control-label" ng-bind="'Опыт Ensemble'|localize">:</label>
								<input class="span12"  type="text" ng-model="page.intern.ensemble" required></input>
								<label class="control-label" ng-bind="'Оценка TET'|localize">:</label>
								<input class="span12"  type="text" ng-model="page.intern.tet" required></input>
								<label class="control-label" ng-bind="'Оценка PET'|localize">:</label>
								<input class="span12"  type="text" ng-model="page.intern.pet.value" required></input>								 
						   </div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<div class="control-group">
			<div class="controls">
				<button type="submit" class="btn btn-success btn-small" ng-bind="page.formBtnSubmitName" ng-disabled="pageForm.$pristine"></button>
			</div>
	 	</div>
	</div>
	</form>
	<div class="span10">
	</div>
</div>
<stialert data="page.alert" span-right="4" span-middle="4" span-left="4"/>

]]></CSP>
</Export>
