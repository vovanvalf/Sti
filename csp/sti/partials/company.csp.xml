<?xml version="1.0" encoding="UTF-8"?>
<Export generator="Cache" version="25">
<CSP name="sti/partials/company.csp" application="/csp/sti/" default="1"><![CDATA[
<div class="row-fluid">
	<div class="span10">
	</div>
	<form name="pageForm" ng-submit="page.submit()" style="margin: 0 0 10px 0;" >
	<div class="span10">
		<div class="accordion">
			<div class="accordion-group">
				<div class="accordion-heading">
					<a class="accordion-toggle" ng-bind="'Компания'|localize"></a>
				</div>
				<div class="accordion-body">
					<div class="accordion-inner">
						 <div class="row-fluid">
							<div class="span4">
								<label class="control-label" ng-bind="'Полное название'|localize">:</label>
								 <input class="span12" type="text" ng-model="page.company.fullName" required></input>
								<label class="control-label" ng-bind="'Аббревиатура'|localize">:</label>
								<input class="span12" type="text" ng-model="page.company.shortName" required></input>
								<label class="control-label" ng-bind="'Адрес'|localize">:</label>
								<input class="span12" type="text" ng-model="page.company.legalAddress" required></input>
					    	</div>
							<div class="span4">
								<label class="control-label" ng-bind="'Email'|localize">:</label>
								<input class="span12" type="text" ng-model="page.company.mailAddress" required></input>
								<label class="control-label" ng-bind="'Web-страница'|localize">:</label>
								<input class="span12" type="text" ng-model="page.company.webSite" required></input>
								<label class="control-label" ng-bind="'Куратор'|localize">:</label>
								<input class="span12" type="text" ng-model="page.company.contact.firstName" required></input>

							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
			</div>
		</div>
		<div class="control-group">
			 <div class="controls">
				   <button type="submit" class="btn btn-success btn-small" ng-bind="page.formBtnSubmitName" ng-disabled="pageForm.$pristine"></button>
			 </div>
		</div>				
	</div>
	</form>
	<div class="span10">
	</div>
</div>
<stialert data="page.alert" span-right="4" span-middle="4" span-left="4"/>
]]></CSP>
</Export>
