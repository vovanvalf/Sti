<Method name="ConvertToProxyObject">
<FormalSpec>idPET:%String,idEng:%String</FormalSpec>
<ReturnType>%ZEN.proxyObject</ReturnType>
<Implementation><![CDATA[
	set proxy = ##class(%ZEN.proxyObject).%New()
	set proxy.id = ..%Id()
	//узнать подробнее о типе "дата" и реализовать вывод на клиенте и в REST
	//set proxy.birthday = $ZDATE(..BirthDay, 3)
	set proxy.firstname = ..FirstName.Value
	set proxy.lastname = ..LastName.Value
	set proxy.middlename = ..MiddleName.Value
	set proxy.phone = ..Phone
	set proxy.email = ..Email
	set proxy.skype = ..Skype
	set proxy.agree = ..MailAgree
	set proxy.cache = ..CacheScience
	set proxy.ensemble = ..EnsembleScience
	set proxy.deepsee = ..DeepSeeScience
	set proxy.tet = ..TET
 	set proxy.pet = ..Pet.ConvertToProxyObject(idPET)
	set proxy.resume = ..Resume
	set proxy.city = ..City.ConvertToProxyObject()
   	set proxy.university = ..University.ConvertToProxyObject()
	set proxy.eng=..engScience.ConvertToProxyObject(idEng)
	quit proxy
]]></Implementation>
</Method>

